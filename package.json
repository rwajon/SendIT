{
  "name": "sendit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "lint": "./node_modules/eslint/bin/eslint.js",
    "test": "mocha --timeout 15000 --compilers js:babel-register tests/**.test.js",
    "build": "babel src/ --presets babel-preset-env --out-dir build/",
    "cover": "istanbul cover _mocha -- --compilers js:babel-register --timeout 15000 tests/**.test.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "start": "node ./build/bin/www.js",
    "devstart": "nodemon ./src/bin/www.js --exec babel-node --presets env"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-routes-versioning": "^1.0.1",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^1.1.0-alpha.1",
    "karma-coverage": "^1.1.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.5",
    "rimraf": "^2.6.2"
  },
  "nodemonConfig": {
    "ignore": [
      "private/*.json",
      "public/js/*.js"
    ]
  }
}